<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ii.shooldemo">
    <!--android:launchMode="singleTask"   横屏显示-->
    <!--android:screenOrientation="landscape"   横屏显示-->

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:launchMode="singleTask"
        android:screenOrientation="landscape"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--注册页面，资源页不需要注册-->
        <activity android:name=".ModuleActivity" />
        <activity android:name=".student.studentActivity" />
        <activity android:name=".myinfo.pinqing" />
        <activity android:name=".myinfo.JiaJiao" />
        <activity android:name=".myinfo.XiuGai" />
        <activity android:name=".xiugai.studengXG" />
        <activity android:name=".shuji.ShuJi" />
        <activity android:name=".myinfo.YueTan" />
        <activity android:name=".wode.wode" />
    </application>
</manifest>